<odoo>

    <record id="messages_view_tree" model="ir.ui.view">
        <field name="model">bot.messages</field>
        <field name="arch" type="xml">
            <tree>
                <field name="message"/>
                <field name="create_date"/>
            </tree>
        </field>
    </record>

    <record id="messages_model_action" model="ir.actions.act_window">
        <field name="name">Message management</field>
        <field name="res_model">bot.messages</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>